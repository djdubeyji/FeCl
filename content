      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "\n",
        "def process_excel(file_path):\n",
        "    # Read the input Excel file\n",
        "    df = pd.read_excel(file_path)\n",
        "\n",
        "    # Ensure column names are as expected\n",
        "    expected_columns = ['app', 'answer_id', 'Req1', 'Req12']\n",
        "    if not all(col in df.columns for col in expected_columns):\n",
        "        raise ValueError(f\"Excel file must contain the columns: {expected_columns}\")\n",
        "\n",
        "    # List of all distinct requirements\n",
        "    requirements = [\n",
        "        'UT1: Older Adults Give And Manage Feedback',\n",
        "        'UT1S1: Express Own Feedback',\n",
        "        'SF: Navigate To SMARTAGE Portal',\n",
        "        'SF: Display Question',\n",
        "        'SF: Skip Question',\n",
        "        'SF: Display Feedback Form',\n",
        "        'SF: Display Application Information',\n",
        "        'SF: Add File',\n",
        "        'UT1S2: Discuss Given Feedback',\n",
        "        'SF: Display Comment',\n",
        "        'SF: Add Comment',\n",
        "        'SF: Add Audio Recording',\n",
        "        'SF: Filter Private Answer And Feedback',\n",
        "        'SF: Display Private Answer And Feedback',\n",
        "        'UT1S3: Review Given Feedback',\n",
        "        'SF: Display History Details',\n",
        "        'SF: Filter History',\n",
        "        'SF: Display History',\n",
        "        'W1: Question View',\n",
        "        'W3: Private Answer And Feedback View',\n",
        "        'W4: Feedback View',\n",
        "        'W5: History View',\n",
        "        'W6: Instruction View',\n",
        "        'W7: Comment View',\n",
        "        'W8: SMARTAGE Portal View',\n",
        "        'W9: Detail View',\n",
        "        'W10: Sidebar View',\n",
        "        'W11: Public Answer And Feedback View',\n",
        "        'NFR: Time Behaviour',\n",
        "        'NFR: User Error Protection',\n",
        "        'NFR: Accessibility',\n",
        "        'NFR: Modifiability',\n",
        "        'NFR: Compatibility',\n",
        "        'NFR: Security',\n",
        "        'NFR: Learnability',\n",
        "        'NFR: Operability',\n",
        "        'NFR: Comfort',\n",
        "        'NFR: Pleasure',\n",
        "        'NFR: Trust',\n",
        "        'NFR: Usefulness',\n",
        "        'Effectiveness',\n",
        "        'Efficiency'\n",
        "    ]\n",
        "\n",
        "    # Get unique application names\n",
        "    applications = df['app'].unique()\n",
        "\n",
        "    # Process each application separately\n",
        "    for app in applications:\n",
        "        # Filter data for the current application\n",
        "        app_data = df[df['app'] == app]\n",
        "\n",
        "        # Create a new DataFrame with requirements as columns\n",
        "        output_df = pd.DataFrame(columns=['answer_id'] + requirements)\n",
        "        output_df['answer_id'] = app_data['answer_id']\n",
        "\n",
        "        # Fill in requirements\n",
        "        for req in requirements:\n",
        "            output_df[req] = app_data.apply(\n",
        "                lambda row: row['answer_id'] if req in (row['Req1'], row['Req12']) else '', axis=1\n",
        "            )\n",
        "\n",
        "        # Save to a new Excel file\n",
        "        output_file_name = f\"{app}_requirements.xlsx\"\n",
        "        output_df.to_excel(output_file_name, index=False)\n",
        "        print(f\"Created: {output_file_name}\")\n",
        "\n",
        "# Path to the uploaded file\n",
        "file_path = '/content/sample_data/Master_Coded_Data.xlsx'\n",
        "process_excel(file_path)\n"
